    lcd.clear();
  lcd.print("T2:");
  lcd.print(ITS2);
  lcd.print("F");
  o=1;
  p=0;
  timediff==0;
  if(ITS2>=i && ITS2<=k) {
    digitalWrite(Pump,HIGH);
  }
